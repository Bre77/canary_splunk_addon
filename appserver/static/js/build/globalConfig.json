{
    "pages": {
        "inputs": {
            "description": "Manage your data inputs", 
            "title": "Inputs", 
            "table": {
                "actions": [
                    "edit", 
                    "enable", 
                    "delete", 
                    "clone"
                ], 
                "header": [
                    {
                        "label": "Name", 
                        "field": "name"
                    }, 
                    {
                        "label": "Interval", 
                        "field": "interval"
                    }, 
                    {
                        "label": "Index", 
                        "field": "index"
                    }, 
                    {
                        "label": "Status", 
                        "field": "disabled"
                    }
                ], 
                "moreInfo": [
                    {
                        "label": "Name", 
                        "field": "name"
                    }, 
                    {
                        "label": "Interval", 
                        "field": "interval"
                    }, 
                    {
                        "label": "Index", 
                        "field": "index"
                    }, 
                    {
                        "label": "Status", 
                        "field": "disabled"
                    }, 
                    {
                        "label": "Debug Mode", 
                        "field": "debug_mode"
                    }
                ]
            }, 
            "services": [
                {
                    "title": "Canary API", 
                    "entity": [
                        {
                            "label": "Name", 
                            "type": "text", 
                            "field": "name", 
                            "required": true, 
                            "help": "Enter a unique name for the data input", 
                            "validators": [
                                {
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores.", 
                                    "type": "regex", 
                                    "pattern": "^[a-zA-Z]\\w*$"
                                }, 
                                {
                                    "maxLength": 100, 
                                    "type": "string", 
                                    "errorMsg": "Length of input name should be between 1 and 100", 
                                    "minLength": 1
                                }
                            ]
                        }, 
                        {
                            "label": "Interval", 
                            "type": "text", 
                            "field": "interval", 
                            "required": true, 
                            "help": "Time interval of input in seconds.", 
                            "validators": [
                                {
                                    "errorMsg": "Interval must be an integer.", 
                                    "type": "regex", 
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$"
                                }
                            ]
                        }, 
                        {
                            "label": "Index", 
                            "type": "singleSelect", 
                            "field": "index", 
                            "defaultValue": "default", 
                            "options": {
                                "createSearchChoice": true, 
                                "endpointUrl": "data/indexes", 
                                "blackList": "^_.*$"
                            }, 
                            "validators": [
                                {
                                    "maxLength": 80, 
                                    "type": "string", 
                                    "errorMsg": "Length of index name should be between 1 and 80.", 
                                    "minLength": 1
                                }
                            ], 
                            "required": true
                        }, 
                        {
                            "label": "Debug Mode", 
                            "help": "For future use.  Not implemented.", 
                            "field": "debug_mode", 
                            "defaultValue": "disabled", 
                            "type": "singleSelect", 
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "label": "Enabled", 
                                        "value": "enabled"
                                    }, 
                                    {
                                        "label": "Disabled", 
                                        "value": "disabled"
                                    }
                                ], 
                                "disableSearch": true
                            }, 
                            "required": true
                        }
                    ], 
                    "name": "canary_api"
                }
            ]
        }, 
        "configuration": {
            "tabs": [
                {
                    "title": "Proxy", 
                    "entity": [
                        {
                            "label": "Enable", 
                            "type": "checkbox", 
                            "field": "proxy_enabled"
                        }, 
                        {
                            "label": "Proxy Type", 
                            "type": "singleSelect", 
                            "field": "proxy_type", 
                            "defaultValue": "http", 
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "label": "http", 
                                        "value": "http"
                                    }, 
                                    {
                                        "label": "socks4", 
                                        "value": "socks4"
                                    }, 
                                    {
                                        "label": "socks5", 
                                        "value": "socks5"
                                    }
                                ], 
                                "disableSearch": true
                            }
                        }, 
                        {
                            "label": "Host", 
                            "type": "text", 
                            "field": "proxy_url", 
                            "validators": [
                                {
                                    "maxLength": 4096, 
                                    "type": "string", 
                                    "errorMsg": "Max host length is 4096", 
                                    "minLength": 0
                                }
                            ]
                        }, 
                        {
                            "label": "Port", 
                            "type": "text", 
                            "field": "proxy_port", 
                            "validators": [
                                {
                                    "type": "number", 
                                    "range": [
                                        1, 
                                        65535
                                    ]
                                }
                            ]
                        }, 
                        {
                            "label": "Username", 
                            "type": "text", 
                            "field": "proxy_username", 
                            "validators": [
                                {
                                    "maxLength": 50, 
                                    "type": "string", 
                                    "errorMsg": "Max length of username is 50", 
                                    "minLength": 0
                                }
                            ]
                        }, 
                        {
                            "label": "Password", 
                            "type": "text", 
                            "field": "proxy_password", 
                            "validators": [
                                {
                                    "maxLength": 8192, 
                                    "type": "string", 
                                    "errorMsg": "Max length of password is 8192", 
                                    "minLength": 0
                                }
                            ], 
                            "encrypted": true
                        }, 
                        {
                            "label": "Remote DNS resolution", 
                            "type": "checkbox", 
                            "field": "proxy_rdns"
                        }
                    ], 
                    "name": "proxy", 
                    "options": {
                        "saveValidator": "function(formData) { if(!formData.proxy_enabled || formData.proxy_enabled === '0') {return true; } if(!formData.proxy_url) { return 'Proxy Host can not be empty'; } if(!formData.proxy_port) { return 'Proxy Port can not be empty'; } return true; }"
                    }
                }, 
                {
                    "title": "Logging", 
                    "entity": [
                        {
                            "label": "Log level", 
                            "type": "singleSelect", 
                            "field": "loglevel", 
                            "defaultValue": "INFO", 
                            "options": {
                                "autoCompleteFields": [
                                    {
                                        "label": "DEBUG", 
                                        "value": "DEBUG"
                                    }, 
                                    {
                                        "label": "INFO", 
                                        "value": "INFO"
                                    }, 
                                    {
                                        "label": "WARNING", 
                                        "value": "WARNING"
                                    }, 
                                    {
                                        "label": "ERROR", 
                                        "value": "ERROR"
                                    }, 
                                    {
                                        "label": "CRITICAL", 
                                        "value": "CRITICAL"
                                    }
                                ], 
                                "disableSearch": true
                            }
                        }
                    ], 
                    "name": "logging"
                }, 
                {
                    "title": "Add-on Settings", 
                    "entity": [
                        {
                            "label": "Canary Domain", 
                            "type": "text", 
                            "field": "canary_domain", 
                            "defaultValue": "", 
                            "help": "The first part of your canary domain.  i.e if your canary domain is https://mycanary.canary.tools then this field would be 'mycanary'", 
                            "validators": [
                                {
                                    "maxLength": 8192, 
                                    "type": "string", 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "minLength": 0
                                }
                            ], 
                            "required": true
                        }, 
                        {
                            "label": "API Key", 
                            "type": "text", 
                            "field": "api_key", 
                            "defaultValue": "", 
                            "help": "API Key from canary dashboard.", 
                            "validators": [
                                {
                                    "maxLength": 8192, 
                                    "type": "string", 
                                    "errorMsg": "Max length of text input is 8192", 
                                    "minLength": 0
                                }
                            ], 
                            "required": true
                        }
                    ], 
                    "name": "additional_parameters"
                }
            ], 
            "description": "Set up your add-on", 
            "title": "Configuration"
        }
    }, 
    "meta": {
        "version": "1.0.0", 
        "apiVersion": "3.0.0", 
        "displayName": "Thinkst Canary AddOn For Splunk", 
        "name": "TA-canary", 
        "restRoot": "TA_canary"
    }
}